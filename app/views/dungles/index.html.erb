<p id="notice"><%= notice %><i class="fa fa-times fa-lg right" aria-hidden="true"></i></p>

<h1>Dungles</h1>

<table class="table">
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Performance</th>
      <th>Ambience</th>
      <th>Scent</th>
      <th>Cleanliness</th>
      <th>Traffic</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dungles.each do |dungle| %>
      <tr>
        <td><%= dungle.title %></td>
        <td><%= dungle.description %></td>
        <td><%= dungle.performance %></td>
        <td><%= dungle.ambience %></td>
        <td><%= dungle.scent %></td>
        <td><%= dungle.cleanliness %></td>
        <td><%= dungle.traffic %></td>
        <td><%= link_to 'Show', user_dungle_path(id: dungle.id) %></td>
        <td><%= link_to 'Edit', edit_user_dungle_path(id: dungle.id) %></td>
        <td><%= link_to 'Destroy', user_dungle_path(id: dungle.id), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dungle', new_user_dungle_path %>
