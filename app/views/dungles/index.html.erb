<p id="notice"><%= notice %></p>

<h1>Dungles</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Description</th>
      <th>Performance</th>
      <th>Ambience</th>
      <th>Scent</th>
      <th>Cleanliness</th>
      <th>Traffic</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dungles.each do |dungle| %>
      <tr>
        <td><%= dungle.user %></td>
        <td><%= dungle.description %></td>
        <td><%= dungle.performance %></td>
        <td><%= dungle.ambience %></td>
        <td><%= dungle.scent %></td>
        <td><%= dungle.cleanliness %></td>
        <td><%= dungle.traffic %></td>
        <td><%= link_to 'Show', dungle %></td>
        <td><%= link_to 'Edit', edit_dungle_path(dungle) %></td>
        <td><%= link_to 'Destroy', dungle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dungle', new_dungle_path %>
